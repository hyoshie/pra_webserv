BIN_DIR	Makefile	/^BIN_DIR = .\/bin$/;"	m
CLIENT_NAME	Makefile	/^CLIENT_NAME = client$/;"	m
CLIENT_OBJS	Makefile	/^CLIENT_OBJS = $(addprefix $(OBJS_DIR)\/, $(notdir $(CLIENT_SRCS:.c=.o)))$/;"	m
CLIENT_SRCS	Makefile	/^CLIENT_SRCS = TCPEchoClient.c \\$/;"	m
DieWithError	src/DieWithError.c	/^void	DieWithError(char *errorMessage)$/;"	f
HandleTCPClient	src/HandleTCPClient.c	/^void	HandleTCPClient(int clntSocket)$/;"	f
MAXPENDING	src/TCPEchoServer.c	/^#define MAXPENDING /;"	d	file:
OBJS_DIR	Makefile	/^OBJS_DIR = .\/obj$/;"	m
PrintSockBufSize	src/TCPEchoServer.c	/^void	PrintSockBufSize(int socket, char *sockKind)$/;"	f
RCVBUFSIZE	src/HandleTCPClient.c	/^#define RCVBUFSIZE /;"	d	file:
RCVBUFSIZE	src/TCPEchoClient.c	/^#define RCVBUFSIZE /;"	d	file:
SERVER_NAME	Makefile	/^SERVER_NAME = server$/;"	m
SERVER_OBJS	Makefile	/^SERVER_OBJS = $(addprefix $(OBJS_DIR)\/, $(notdir $(SERVER_SRCS:.c=.o)))$/;"	m
SERVER_SRCS	Makefile	/^SERVER_SRCS = TCPEchoServer.c \\$/;"	m
SRCS_DIR	Makefile	/^SRCS_DIR = .\/src$/;"	m
main	src/TCPEchoClient.c	/^int	main(int argc, char *argv[])$/;"	f
main	src/TCPEchoServer.c	/^int	main(int argc, char *argv[])$/;"	f
